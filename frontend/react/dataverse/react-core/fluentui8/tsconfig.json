{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@ianus-core/*": ["./ianus-core/*"]
      },
      "typeRoots": ["node_modules/@types"],
      "jsx": "react",
      "target": "es6",
      "module": "es2020",
      "moduleResolution": "node",
      "lib": ["ES2020", "DOM"],
      "strict": true,
      "strictPropertyInitialization": false
    },
    "exclude": [
      "./node_modules"
    ]
}